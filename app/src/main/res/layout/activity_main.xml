<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/card_round_white"
    android:orientation="vertical"

    tools:context=".MainActivity">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_gravity="center"
                android:layout_height="80dp">

                <LinearLayout
                    android:id="@+id/layoutParent"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    android:minHeight="60dp"
                    android:orientation="horizontal" />


            </androidx.cardview.widget.CardView>


            <TextView
                android:id="@+id/test1"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/test2"
                style="@style/textStyle"
                android:layout_width="1080dp"
                android:layout_height="40dp"
                android:background="#d1F5F7FA"
                android:text="333"
                android:textSize="14dp" />

            <TextView
                android:id="@+id/test3"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:textSize="14px" />

            <TextView
                android:id="@+id/test4"
                style="@style/textStyle"

                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test5"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/alibaba_puhui_55_regular_55_regular"
                android:textSize="12sp" />


            <TextView
                android:id="@+id/test6"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/alibaba_puhui_65_medium_65_medium"
                android:textSize="14sp" />


            <TextView
                android:id="@+id/test7"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/alibaba_puhui_55_regular_85_bold" />

            <TextView
                android:id="@+id/test8"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/test9"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test10"
                style="@style/textStyle"

                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/test11"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test12"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/test13"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test14"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test15"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test16"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/test17"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test18"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test19"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test20"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/test21"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/test22"
                style="@style/textStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="24dp"
                        android:text="你好点的333"
                        android:textSize="16sp" />


                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="你好点的"
                        android:textSize="24sp" />


                </LinearLayout>


            </LinearLayout>


            <EditText
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_margin="12dp" />

            <CheckBox
                android:id="@+id/switchComp"
                android:layout_width="42dp"
                android:layout_height="21dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/switch_selector"
                android:button="@null"
                android:gravity="center|right" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:src="@drawable/switch_selector" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/switchWidget"
                android:layout_width="42dp"
                android:layout_height="21dp"
                android:layout_marginTop="16dp"
                android:gravity="center|right"
                android:thumb="@null"
                android:track="@null" />


            <CheckBox
                android:id="@android:id/checkbox"
                android:layout_width="42dp"
                android:layout_height="22dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/switch_selector"

                android:button="@null"
                android:gravity="center|right" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="80dp"
                android:orientation="vertical">

                <com.bella.android_demo_public.view.FdeSeekBar
                    android:id="@+id/fdeSeekBar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:max="8"
                    android:minHeight="80dp"
                    android:tickMark="@drawable/multi_tick_mark" />


            </LinearLayout>


            <androidx.appcompat.widget.Toolbar
                android:id="@+id/action_bar"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:background="#00ff00"
                android:visibility="invisible"
                app:title="333" />


            <androidx.coordinatorlayout.widget.CoordinatorLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

            </androidx.coordinatorlayout.widget.CoordinatorLayout>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:background="#ff0000"
                    android:paddingHorizontal="16dp"
                    android:visibility="gone" />


            </RelativeLayout>

            <Toolbar
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:tag="drag"
                android:visibility="gone">

                <include layout="@layout/layout_settings_title_bar" />
            </Toolbar>


            <Toolbar
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:tag="drag"
                android:visibility="gone">

                <include layout="@layout/layout_settings_title_bar" />
            </Toolbar>


            <TextView
                android:id="@+id/txtTitle"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:layout_marginTop="46dp"
                android:gravity="center|left"
                android:paddingHorizontal="32dp"
                android:text="111"
                android:textColor="#262626"
                android:textSize="14sp"
                android:textStyle="bold"
                android:visibility="visible" />


            <TextView
                android:id="@+id/testC"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="testClick"
                android:text="testClick333" />


        </LinearLayout>


    </ScrollView>


</LinearLayout>